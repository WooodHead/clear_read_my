{
"manifest_version":2,
"name":"Clear Read My",
"version":"1",

"background":{
"scripts":["background.js"]
},

"content_scripts":[
{
"js":["jquery-2.1.3.min.js","content.js"],
"css":["style.css"],
"matches":["<all_urls>"]
}
],

"browser_action":{
"default_icon":{
"19":"images/icon19.png"}
},

"permissions":["tabs","activeTab"]
}